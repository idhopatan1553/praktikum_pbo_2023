package L56_umurBarangAntik;

public class BarangAntik {
    private int umur;

    // Konstruktor
    public BarangAntik(int umur) {
        this.umur = umur;
    }

    // Metode untuk menampilkan umur barang antik
    public void tampilUmur() {
        System.out.println("Umur barang antik: " + umur + " tahun.");
    }
}